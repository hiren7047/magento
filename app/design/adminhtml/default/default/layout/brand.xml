<?xml version="1.0" encoding="UTF-8"?>
<layout>
	<brand_adminhtml_brand_attribute_edit>
		<reference name="content">
			<block type="brand/adminhtml_brand_attribute_edit" name="brandAttributeEdit"/>
		</reference>
		 <reference name="left">
            <block type="brand/adminhtml_brand_attribute_edit_tabs" name="brand_edit_tabs"></block>
        </reference>
        <reference name="js">
            <block type="adminhtml/template" name="attribute_edit_js" template="brand/attribute/js.phtml"></block>
        </reference>
	</brand_adminhtml_brand_attribute_edit>
	
	<brand_adminhtml_brand_edit>
		<reference name="content">
			<block type="brand/adminhtml_brand_edit" name="brandEdit"/>
		</reference>
		<reference name="left">
			<block type="brand/adminhtml_brand_edit_Tabs" name="brandEditTabs"/>
		</reference>
		<reference name="js">
            <block type="adminhtml/catalog_product_edit_js" template="catalog/product/js.phtml" name="catalog_product_js"></block>
            <block type="core/template" template="catalog/wysiwyg/js.phtml"/>
        </reference>
	</brand_adminhtml_brand_edit>	
</layout>